.index_swiper swiper {
  height: 340rpx;
}
.index_nav_cate {
  display: flex;
}
.index_nav_cate navigator {
  flex: 1;
  padding: 30rpx;
}
.index_floor .floor_item .floor_title {
  padding: 10rpx 0;
}
.index_floor .floor_item .floor_list {
  padding: 0 10rpx;
  overflow: hidden;
}
.index_floor .floor_item .floor_list .goods_item {
  width: 33.3%;
  float: left;
}
.index_floor .floor_item .floor_list .goods_item:nth-last-child(-n+4) {
  height: 207.97413793rpx;
  border-left: 10rpx solid #fff;
}
.index_floor .floor_item .floor_list .goods_item:nth-last-child(-n+4) image {
  height: 100%;
}
.index_floor .floor_item .floor_list .goods_item:nth-last-child(-n+2) {
  border-top: 10rpx solid #fff;
}
